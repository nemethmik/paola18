{
  "name": "paola18",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && copyfiles products.json people.json dist",
    "serve": "vite preview",
    "fix": "eslint src --fix",
    "servor": "npx servor dist index.html 8080 --browse --reload",
    "ui5:dialog": "tsc ./node_modules/@ui5/webcomponents/src/Dialog.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:checkbox": "tsc ./node_modules/@ui5/webcomponents/src/CheckBox.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:datepicker": "tsc ./node_modules/@ui5/webcomponents/src/DatePicker.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:switch": "tsc ./node_modules/@ui5/webcomponents/src/Switch.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:popup": "tsc ./node_modules/@ui5/webcomponents/src/Popup.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:table": "tsc ./node_modules/@ui5/webcomponents/src/Table.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:tablerow": "tsc ./node_modules/@ui5/webcomponents/src/TableRow.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:tablecolumn": "tsc ./node_modules/@ui5/webcomponents/src/TableColumn.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:tablecell": "tsc ./node_modules/@ui5/webcomponents/src/TableCell.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:busyindicator": "tsc ./node_modules/@ui5/webcomponents/src/BusyIndicator.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types",
    "ui5:toast": "tsc ./node_modules/@ui5/webcomponents/src/Toast.js --declaration --allowJs --emitDeclarationOnly --outDir ./src/ui5types"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "typescript": "^4.3.5",
    "vite": "^2.5.4"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.11.0",
    "@ui5/webcomponents": "^1.0.0-rc.15",
    "@vaadin/vaadin-button": "^21.0.1",
    "@vaadin/vaadin-grid": "^21.0.1",
    "lit": "^2.0.0-rc.4",
    "lodash-es": "^4.17.21"
  }
}
